{
  "name": "scss-boilerplate",
  "version": "1.0.0",
  "description": "A sass boilerplate based on the 7-1 pattern with some mods.",
  "main": "index.js",
  "scripts": {
    "test": "npm run test",
    "build": "scss-lint ./ && sass --style expanded ./scss/main.scss:./compiled/main.css",
    "watch": "scss-lint ./ && sass --watch --style expanded  ./scss/main.scss:./compiled/main.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sturdynut/scss-boilerplate.git"
  },
  "keywords": [
    "scss",
    "css",
    "architecture"
  ],
  "author": "Matti Salokangas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sturdynut/scss-boilerplate/issues"
  },
  "homepage": "https://github.com/sturdynut/scss-boilerplate#readme"
}
